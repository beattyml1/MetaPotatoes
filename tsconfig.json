    "compilerOptions": {
        "module": "commonjs",
        "jsx": "react",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "out": "built/local/tsc.js",
        "sourceMap": true,
        "outDir": "built/bin"
    },
    "filesGlobal": [
        "*.ts",
        "*.tsx"
    ]
}
